  <h1 class="title"> Versus </h1>

  <div class="d-flex justify-content-center flex-column align-items-sm-center flex-sm-row">
    <div class="d-flex align-items-center">
      <div class="align-self-center">
        <ol>
          <% grades.filter(grade => grade.movieID.decade === 80).forEach(grade => { %>
          <li> <%= grade.movieID.title %></li>
          <% }) %>
        </ol>
      </div>
      <div class="align-self-center">
        <ol>
          <% grades.filter(grade => grade.movieID.decade === 90).forEach(grade => { %>
          <li>
            <%= grade.movieID.title %>
          </li>
          <% }) %>
        </ol>
      </div>
